{
  "name": "translime-plugin-example",
  "version": "1.0.0",
  "description": "a plugin example",
  "license": "MIT",
  "private": true,
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "files": [
    "**"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build:plugin": "vite build",
    "build:ui": "vite build -c ui.vite.config.js",
    "build": "yarn build:plugin && yarn build:ui",
    "build:test": "yarn build:plugin && yarn build:ui --mode development"
  },
  "devDependencies": {
    "vite": "^2.7.2",
    "vite-plugin-vue2": "^1.9.3"
  },
  "plugin": {
    "title": "plugin title",
    "description": "plugin description",
    "icon": "./icon.png",
    "windowUrlxx": "dist/ui/index.html",
    "ui": "dist/ui.umd.js"
  }
}
